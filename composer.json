{
    "name": "giorgosstab/bootstrap-starter",
    "type": "project",
    "description": "Bootstrap starter for static web pages.",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "jenssegers/blade": "^2.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "files": [
            "app/Helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
